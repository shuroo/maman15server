drop database IF EXISTS maman_db;
CREATE DATABASE IF NOT EXISTS maman_db;
USE maman_db;
CREATE TABLE IF NOT EXISTS Clients (clientID BINARY( 16 ) primary key not null,
            UserName varchar(256),
            PublicKey  VARBINARY(161) ,
            LastSeen DATETIME
            );
CREATE TABLE IF NOT EXISTS Messages ( ToClient BINARY( 16 ) ,
            FromClient BINARY( 16 ) ,
            Type varchar(1),
            Content Blob,
            FOREIGN KEY (ToClient) REFERENCES Clients(clientID),
            FOREIGN KEY (FromClient) REFERENCES Clients(clientID)
            );