drop database IF EXISTS maman_db;
CREATE DATABASE IF NOT EXISTS maman_db;
USE maman_db;
CREATE TABLE IF NOT EXISTS Clients (clientID varchar(16) primary key not null,
            UserName varchar(255),
            PublicKey varchar(160),
            LastSeen DATETIME
            );
CREATE TABLE IF NOT EXISTS Messages ( ToClient varchar(16),
            FromClient varchar(16),
            Type varchar(1),
            Content Blob,
            FOREIGN KEY (ToClient) REFERENCES Clients(clientID),
            FOREIGN KEY (FromClient) REFERENCES Clients(clientID)
            );